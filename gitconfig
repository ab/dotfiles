[user]
	name = Andy Brody
	email = andy@abrody.com
[alias]
	stat = status
	co = checkout
	cp = cherry-pick
	fshow = ! sh -c 'git show --pretty="format:" --name-only $1 | grep -v "^$" | uniq | sed -e "s#^#`git rev-parse --show-toplevel`/#"' -
	vim   = ! sh -c 'vim `git fshow $1`' -
	edit  = ! sh -c '$EDITOR `git fshow $1`' -
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = /home/andy/.gitignore_global
[push]
	default = tracking
