# ===== Harvard Tunnels =====
host oz-tunnel
hostname localhost
hostkeyalias oz
port 2222
user andy

host zara-tunnel
hostname localhost
hostkeyalias zara
port 2220
user andy

host eggplant-tunnels
hostname eggplant.selfip.net
user root
BatchMode yes
ExitOnForwardFailure yes
LocalForward 2222 192.168.1.22:22
LocalForward 2220 192.168.1.20:22

host eggplant
hostname eggplant.selfip.net
user root

