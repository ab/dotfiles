# ====== Globals ======

Host *
  # connection sharing
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h-%p

  # keepalive
  ServerAliveInterval 30
  ServerAliveCountMax 4

  # no gssapi
  # this might be overriden in /etc/ssh/config... not sure how it works
  GSSAPIAuthentication no

